        "email": {
            "type": "string",
            "title": "Email address",
            "minLength": 1,
            "maxLength": 250
        },
        "lastName": {
            "type": "string",
            "title": "Last Name",
            "minLength": 1,
            "maxLength": 250
        },
        "firstName": {
            "type": "string",
            "title": "First Name",
            "minLength": 1,
            "maxLength": 250
        },
        "password": {
            "type": "string",
            "title": "Password",
            "minLength": 8,
            "maxLength": 250
        }
